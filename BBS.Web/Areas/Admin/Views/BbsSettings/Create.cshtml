@model BbsSetting
@{
    ViewData["Title"] = "Create Setting";
}

<h1>Create Setting</h1>
<form asp-action="Create" method="post">
    <div class="mb-3">
        <label asp-for="AllowedFileExtensions" class="form-label"></label>
        <input asp-for="AllowedFileExtensions" class="form-control" />
        <span asp-validation-for="AllowedFileExtensions" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
    <a asp-action="Index" class="btn btn-secondary">Back to List</a>
</form>
